<UserControl x:Class="Zengo.WP8.FAS.Controls.WaveCircleControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="480" d:DesignWidth="480">
    
    <Grid x:Name="LayoutRoot" Width="30" Height="30" Background="Transparent" >
    	
		<Ellipse Name="Circle" Fill="#FFBECACB" HorizontalAlignment="Left" Stroke="Black" VerticalAlignment="Top" Width="30" Height="30" StrokeThickness="0"/>
                
    </Grid>
    
</UserControl>